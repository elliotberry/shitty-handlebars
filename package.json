    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "env": {
      "es2021": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  },
  "files": [
    "./*.js",
    "package.json",
    "README.md"
  ],

  "author": "elliot",
  "license": "The Unlicense",
  "devDependencies": {
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1"
  }
}
