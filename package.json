{
  "name": "shitty-handlebars",
  "version": "1.0.2",
  "main": "./index.js",
  "type": "module",
  "scripts": {
    "test": "node index.test.js"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true
